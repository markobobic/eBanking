@model EBanking.Models.Uplatnica
@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Uplatnica</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Uplatilac
        </dt>

        <dd>
            @Model.Uplatilac
        </dd>
        <dt>
            Svrha uplate
        </dt>

        <dd>
            @Model.SvrhaUplate
        </dd>
        <dt>
            Racun
        </dt>

        <dd>
            @Model.Racun.BrojRacuna
        </dd>

    </dl>

    <form method="post">
        <input type="hidden" value="@Model.Racun.Id" name="Racun.Id" />
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            <a href="/Uplatnica?id=@Model.Racun.Id">Back to list</a>
        </div>
    </form>
</div>




